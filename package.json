{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "pages:dev": "wrangler pages dev --proxy 8000 --compatibility-flag=nodejs_compat --compatibility-date=2023-04-04 --kv=MAIN -- npm run dev",
    "build": "tsc && vite build",
    "lint": "npm run eslint && npm run prettier",
    "lint:write": "npm run eslint:write && npm run prettier:write",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:write": "eslint src --ext ts,tsx --report-unused-disable-directives --fix",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx,css,md,json,html}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,css,md,json,html}\"",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "1.7.14",
    "@heroicons/react": "2.0.17",
    "axios": "1.4.0",
    "browser-image-compression": "^2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.1",
    "swr": "2.1.5",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230419.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-react-swc": "3.3.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.1",
    "husky": "8.0.3",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.2.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "wrangler": "2.20.0"
  }
}
