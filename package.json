{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "pages:dev": "wrangler pages dev --port 8788 --proxy 8000 --compatibility-date=2023-04-04 --kv=MAIN --r2=IMAGES -- npm run dev",
    "build": "tsc && vite build",
    "build:functions": "cd functions && tsc",
    "lint": "npm run eslint && npm run prettier",
    "lint:write": "npm run eslint:write && npm run prettier:write",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:write": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "prettier:write": "prettier --write \"./**/*.{ts,tsx,css,md,json,html}\"",
    "prettier": "prettier --check \"./**/*.{ts,tsx,css,md,json,html}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "e2e:test:ci": "node scripts/start-playwright.mjs",
    "e2e:test": "playwright test --project chromium",
    "e2e:watch:chromium": "playwright test --ui --project chromium",
    "e2e:watch:firefox": "playwright test --ui --project firefox",
    "e2e:open": "playwright open http://localhost:8788",
    "ct:test": "playwright test -c playwright-ct.config.ts --project chromium",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@tanstack/react-query": "4.35.3",
    "axios": "1.5.0",
    "browser-image-compression": "2.0.2",
    "react": "18.3.1",
    "react-colorful": "5.6.1",
    "react-dom": "18.3.1",
    "react-popper": "2.3.0",
    "react-router-dom": "6.15.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230904.0",
    "@playwright/experimental-ct-react": "1.37.1",
    "@playwright/test": "1.37.1",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-tailwindcss": "3.13.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "pages-vite-early-hints-plugin": "0.2.0",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.4",
    "wait-on": "7.0.1",
    "wrangler": "3.7.0"
  }
}
