{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "pages:dev": "wrangler pages dev --proxy 8000 --compatibility-date=2023-04-04 --kv=MAIN --r2=IMAGES -- npm run dev",
    "build": "tsc && vite build",
    "build:functions": "cd functions && tsc",
    "lint": "npm run eslint && npm run prettier",
    "lint:write": "npm run eslint:write && npm run prettier:write",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:write": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "prettier:write": "prettier --write \"./**/*.{ts,tsx,css,md,json,html}\"",
    "prettier": "prettier --check \"./**/*.{ts,tsx,css,md,json,html}\"",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "1.7.14",
    "@heroicons/react": "2.0.18",
    "@tanstack/react-query": "4.29.12",
    "axios": "1.4.0",
    "browser-image-compression": "2.0.2",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "react-router-dom": "6.11.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230518.0",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vitejs/plugin-react-swc": "3.3.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.1",
    "husky": "8.0.3",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "3.0.1"
  }
}
