{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "pages:dev": "wrangler pages dev --port 8788 --proxy 8000 --compatibility-date=2023-04-04 --kv=MAIN --r2=IMAGES -- npm run dev",
    "build": "tsc && vite build",
    "build:functions": "cd functions && tsc",
    "lint": "npm run eslint && npm run prettier",
    "lint:write": "npm run eslint:write && npm run prettier:write",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:write": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "prettier:write": "prettier --write \"./**/*.{ts,tsx,css,md,json,html}\"",
    "prettier": "prettier --check \"./**/*.{ts,tsx,css,md,json,html}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "e2e:test:ci": "node scripts/start-playwright.mjs",
    "e2e:test": "playwright test --project chromium",
    "e2e:watch:chromium": "playwright test --ui --project chromium",
    "e2e:watch:firefox": "playwright test --ui --project firefox",
    "e2e:open": "playwright open http://localhost:8788",
    "ct:test": "playwright test -c playwright-ct.config.ts --project chromium",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "@heroicons/react": "2.0.18",
    "@tanstack/react-query": "4.29.15",
    "axios": "1.4.0",
    "browser-image-compression": "2.0.2",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "react-router-dom": "6.14.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230518.0",
    "@playwright/experimental-ct-react": "1.35.1",
    "@playwright/test": "1.35.1",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.1",
    "eslint-plugin-tailwindcss": "3.12.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "pages-vite-early-hints-plugin": "0.2.0",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2",
    "wait-on": "7.0.1",
    "wrangler": "3.1.1"
  }
}
